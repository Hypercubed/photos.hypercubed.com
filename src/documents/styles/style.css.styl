table
	tfoot, tbody
		th
			border-top 1px solid #DDD
			
.carousel-inner
	img
		max-width 100%
		max-height 100%
		
#set-carousel
	width 100%
	height 660px
	background #333
	text-align: center
	
#set-indicators
	position: absolute
	z-index: 100
	left: 50%
	bottom 0
	padding-bottom 10px
	
.carousel-indicators
	position: relative
	left -50%
	top 0
	
.carousel-inner > .item
	text-align center

.carousel-inner > .item > a > img
	display inline
	line-height 1